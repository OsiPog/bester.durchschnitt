{
	"manifest_version": 2,
	"name": "bester.durchschnitt",
	"version": "1.0.11",
	
	"content_scripts": [
		{
			"matches": ["https://beste.schule/*"],
			"js": ["/content_script.js"]
		}
	],
	
	"browser_action": {
		"browser_style": true,
		"default_icon":{
			"48": "/icon.png"
		},
		"default_title": "bester.durchschnitt",
		"default_popup": "/popup.html"
	}
}

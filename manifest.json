{
	"manifest_version": 3,
	"name": "bester.durchschnitt",
	"version": "1.0.22",
	
	"content_scripts": [
		{
			"matches": [
				"https://beste.schule/*",
				"https://einfache.schule/*"
			],
			"js": ["/content_script.js"]
		}
	],
	
	"action": {
		"browser_style": true,
		"default_icon":{
			"48": "/icon.png"
		},
		"default_title": "bester.durchschnitt",
		"default_popup": "/popup.html"
	}
}
